title: RC example with startup, using vsrc_x
# used for coverage report

begin_circuit
#  eelement name=v1 type=vsrc_dc p=b n=a vdc=10
   xelement type=const y=y k=10
   eelement name=v1 type=vsrc_x  p=b n=a x_in=y
   eelement name=r1 type=r p=b n=c r=1k
   eelement name=c1 type=c p=c n=a c=1u v0=5

   eelement name=r2 type=r p=c n=d r=1k
   eelement name=c2 type=c p=d n=a c=1u v0=2

   ref_node=a
   outvar:
+    vb=nodev_of_b
+    vc=nodev_of_c
+    vd=nodev_of_d
+    i_r1=i_of_r1
+    i_r2=i_of_r2
end_circuit

begin_solve
   solve_type=startup
