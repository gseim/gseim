ebe name=voltmeter
# ideal voltmeter without feedback
Jacobian: constant
nodes: p n
state_vars:
aux_vars:
aux_vars_startup:
x_vars:
iparms:
sparms:
rparms:
stparms:
igparms:
outparms: v
n_f=2
f_1:
f_2:
n_g=0
n_h=2
h_1:
h_2:
C:
variables:
source:
   if (G.flags[G.i_one_time_parms]) {
     return;
   }
   if (G.flags[G.i_dc] || G.flags[G.i_trns]) {
     if (G.flags[G.i_function]) {
       X.f[nf_1] = 0.0;
       X.f[nf_2] = 0.0;
     }
     return;
   }
   if (G.flags[G.i_startup]) {
     if (G.flags[G.i_function]) {
       X.h[nh_1] = 0.0;
       X.h[nh_2] = 0.0;
     }
     return;
   }
   if (G.flags[G.i_outvar]) {
     X.outprm[no_v] = X.val_nd[nnd_p]-X.val_nd[nnd_n];
     return;
   }
endC
endebe
